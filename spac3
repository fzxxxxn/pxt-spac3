//% color="#FFAB19"

namespace spac3 {
    //% block
  
    export function group() {

    }
    //% block
    
    
    export function line(block: number){
        
    }
    
       //% block

    export function move(x: number, y: number, z: number){}
    //% block="%p0=minecraftCreatePosition| %p1=minecraftCreatePosition| %p2=minecraftCreatePosition||"
    //% inlineInputMode=external
    export function moveto(p0: Position,p1: Position,p2: Position){
        blocks.clone(
        p0,
        p1,
        p2,
        CloneMask.Replace,
        CloneMode.Move
    );
    }
 
}
